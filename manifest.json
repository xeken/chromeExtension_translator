{
  "name": "병쨩변역",
  "description": "이러쿵 저러쿵",
  "version": "0.1",
  "browser_action": {
    "default_popup": "popup.htm",
    "default_icon": "icon.png"
  },
  "permissions": ["contextMenus", "storage", "tabs"],
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "css": ["content.css"]
  }],
  "background": {
    "matches": ["http://*/*", "https://*/*"],
    "persistent": false,
    "scripts": ["background.js", "jquery-3.4.0.min.js"]
  },
  "manifest_version": 2
}
